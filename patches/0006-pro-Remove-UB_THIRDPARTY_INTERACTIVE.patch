--- a/OpenBoard.pro
+++ b/OpenBoard.pro
@@ -86,7 +86,6 @@ UB_ETC.files = resources/etc
 UB_I18N.files = resources/i18n/*.qm
 UB_LIBRARY.files = resources/library
